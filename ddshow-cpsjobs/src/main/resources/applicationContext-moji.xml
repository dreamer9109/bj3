<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
      http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id="laifengFileSystemService" class="com.laifeng.fs.mogile.service.impl.PackageLaifengFileSystemServiceImpl">
        <constructor-arg index="0" value="${moji.tracker.address}"/>
        <constructor-arg index="1" value="${moji.domain}"/>
        <constructor-arg index="2" value="${moji.class}"/>
    </bean>

</beans>